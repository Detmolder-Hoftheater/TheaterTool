                <row>
                    <cell><date when="1825-11-08">Dienstag den 8ten Nov.</date></cell>
                    <cell>Zur Einweihung des Hoftheaters in Detmold: <rs type="work">Prolog mit Gesang</rs>. Gedichtet von <persName>F. Falkmann</persName>, in Musik gesetzt von <persName>H. Neumann</persName>. Hierauf: <rs type="work">Titus der Gütige</rs>, nach <persName>Metastasios</persName> <rs type="work">Clemenza di Tito. Oper in 2 Abtheilungen</rs>, Musik von <persName>Mozart</persName>.</cell>
                </row><!-- TZ 24 -->
                <row>
                    <cell><date when="1825-11-11">Donnerstag den 11ten Nov.</date></cell>
                    <cell><rs type="work">Der Empfehlungsbrief. Lustspiel in 4 Acten</rs>, von <persName>Doctor Carl Töpfer</persName></cell>
                </row><!-- Tz 25 -->
                                <row>
                                       <cell><date when="1825-11-13">Sonntag den 13ten November.</date></cell>
                    <cell><rs type="work">Preciosa. Romantisches Schauspiel in 4 Akten mit Gesang und Tanz</rs>, von <persName>Wolff</persName>, Musik von <persName>Carl Maria von Weber</persName>.</cell>
                </row>
                <row>
                    <cell><date when="1825-11-14">Montag den 14ten -</date></cell>
                    <cell><rs type="work">Die Advokaten. Schauspiel in 5 Akten</rs> von <persName>Iffland</persName></cell>
                </row>
                <row>
                    <cell><date when="1825-11-15">Dienstag den 15ten -</date></cell>
                    <cell><rs type="work">Johann von Paris. Oper in 2 Akten mit Ballet</rs> getanzt von Kindern.</cell>
                </row>
                <row>
                    <cell><date when="1825-11-17">Donnerstag den 17ten -</date></cell>
                    <cell><rs type="work">Die Mohrin. Schauspiel in 4 Akten</rs> von <persName>Ziegler</persName></cell>
                </row>
                <row>
                    <cell><date when="1825-11-18">Freitag den 18ten -</date></cell>
                    <cell><rs type="work">Der Schnee, oder: Der neue Eginhard. Oper in 4 Abtheilungen</rs>, Musik von <persName>Auber</persName></cell>
